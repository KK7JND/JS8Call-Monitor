[GREEN]
# Used with INFO messages.
search = PIR1=G
map = MODE:JT4A
color = #00f900
aprs = ['\','W']

[YELLOW]
# Used with INFO messages.
search = PIR1=Y
map = MODE:JT4B
color = #fffb00
aprs = ['\','U']

[RED]
# Used with INFO messages.
search = PIR1=R
map = MODE:JT4C
color = #ff2600
aprs = ['/',':']

[UNKNOWN]
# Used with INFO messages.
search = PIR1=U
map = MODE:JT4D
color = #ff9300
aprs = ['\','0']

[NOPIR]
# Used to indicate that an INFO message was found, but no PIR was detected.
map = MODE:JT4E
color = #00fdff
aprs = ['\','.']

[NOINFO]
# Used to indicate no INFO message detected.
map = MODE:JT4F
color = #ff40ff
aprs = ['\','!']

[STATUS]
# Used to indicate STATUS message detected.
map = MODE:JT4G
color = #942192
aprs = ['\',')']

